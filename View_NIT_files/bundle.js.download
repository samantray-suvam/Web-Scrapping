function scalePrint() {
  var tables = document.querySelectorAll('table'); //Getting All the tables on the page

  var windowWidth = window.innerWidth - window.innerWidth * 0.22; //Getting width of the viewport in which we have to scale all the tables

  for (var i = 0; i < tables.length; i++) {
    //Going through all the tables to scale it
    var table = tables[i]; // Getting Table

    var tableWidth = table.scrollWidth; // Getting actual width of the table

    if (tableWidth > window.innerWidth) {
      //Scale only if table width is greater then viewport width
      var scale = windowWidth / tableWidth; //Creating scale factor by dividing Viewport width to the overflowing table width

      scaleTable(table, scale); //Calling function to scale the table with its scalefactor.
    }
  }
}

